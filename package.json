{
  "name": "next-pace-development",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:blog:dev && vite",
    "dev:blog": "cd blog && hugo server --port 1314 --bind 0.0.0.0",
    "build": "npm run build:blog:prod && vite build",
    "build:blog": "npm run build:blog:prod",
    "build:blog:dev": "cd blog && hugo --minify --baseURL /",
    "build:blog:prod": "cd blog && hugo --minify --baseURL https://nextpace.dev",
    "preview": "vite preview"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "sass": "^1.94.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
